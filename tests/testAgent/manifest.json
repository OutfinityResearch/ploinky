{
  "container": "node:20-bullseye",
  "start": "/code/start_script.sh",
  "agent": "node /code/server.js",
  "cli": "/code/testSSOParams.sh",
  "enable": [
    "testAgentDepGlobal global",
    "testAgentDepDevel devel testRepo",
    "globalAgentForAlias global as globalAgentAlias"
  ],
  "volumes": {
    "test-volumes/data": "/mnt/test-data"
  },
  "ports": [
    "__HOST_PORT__:7000"
  ],
  "profiles": {
    "default": {
      "env": {
        "FAST_TEST_MARKER": "fast-suite",
        "MY_TEST_VAR": "hello-manifest"
      },
      "install": "echo 'install_ok' > \"$WORKSPACE_PATH/install_marker.txt\""
    }
  }
}
